body
  padding 20px
  font-family arial

th
  font-weight normal

.dashboard table
  position absolute
  top 0px
  bottom 0px
  width 90%

.status
  position absolute
  font-size small
  top 0
  right 5
  margin-top 0

.status .updated
  padding-left 5

.tops
  width 90%

.dashboard .response-time, .dashboard .error-percentage
  width 45%

.dashboard .mark
  stroke-width 1
  fill none

.dashboard .mark.failed
  stroke maroon

.dashboard .mark.passed
  stroke steelblue

.graph
  border 1px dotted grey
  padding 30px

path
  fill none

.tile
  cursor hand

.boxplot
  shape-rendering crispEdges
  stroke steelblue

.boxplot.percentiles
  stroke-width 5px

.boxplot.percentiles:hover
  cursor hand
  stroke-width 7px
  stroke lightblue

.boxplot.median
  stroke-width 1px
  stroke white

.error-percentage path
  stroke maroon

.axis text
  font-size 11px

.axis path, .axis line
  fill none
  stroke grey
  shape-rendering crispEdges

.report .mark
  stroke-width 1
  fill none

.report .mark.failed
  stroke maroon

.report .mark.passed
  stroke steelblue

.report .testrun
  padding-left 1em

.report .sample span
  padding-left 0.5em
  color maroon

.tops .response-time
  color maroon

.tops .response-time .elapsedTimeStr, .tops .response-time .responseCode
  text-align right

.tops .response-time a
  margin-left 20


